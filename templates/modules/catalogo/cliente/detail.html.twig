<div class="row">
    <div class="col-6">
        <h5 class="mb-0"><b>Clave:</b> {{ item.clave | default('') }}</h5>
        <h5 class="mb-0"><b>Nombre:</b> {{ item.nombre | default('') }}</h5>
        <h5 class="mb-0"><b>Tipo de identificador fiscal:</b> {{ item.tipoIdentificadorFiscal.clave | default('') }}
        </h5>
    </div>
    <div class="col-6">
        <h5 class="mb-0"><b>Identificador fiscal:</b> {{ item.identificadorFiscal | default('') }}</h5>
        {% if '1' == item.activo %}
            <h5 class="mb-0"><b>Estado:</b> Habilitado</h5>
        {% else %}
            <h5 class="mb-0"><b>Estado:</b> Deshabilitado</h5>
        {% endif %}
    </div>
</div>

